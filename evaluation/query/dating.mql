MATCH (N1100) =[DATA_TEST ?e (person {?p == age and ?x == pos_x and ?y == pos_y})/ ((:A {true})/(person {true}))*/(:A {true}) /(person {
               ?p * 0.5 + 7 <= age
               and  (?x - pos_x) + (?y - pos_y) <= 20 and
                       (?x - pos_x) + (pos_y - ?y) <= 20 and
                       (pos_x - ?x) + (?y - pos_y) <= 20 and
                       (pos_x - ?x) + (pos_y - ?y) <= 20 and status == "single"
               })]=>(?to)
RETURN *
